@article{Malthe-Sorenssen1999,
	Author = {Anders Malthe-S\o renssen},
	Journal = {Phys. Rev. E},
	Month = apr,
	Number = 4,
	Optnote = {In this paper, a version of the oslo model is studied where the critical slopes are continuous random variables chosen from the interval between 1 and 2. The relaxation rules are as in the original model, but the drive is different: A driving time step corresponds to increasing the slopes at all sites by dz until one site reaches its critical slope value and relaxes, triggering an avalanche (similar to OFC dynamics). In this case, it turns out that, although dz \propto L^{-1}, the energy added to the pile in each step scales as L^{2}. This can be calculated by considering the potential energy of a wedge added onto a slope (\propto L^3 dz). Thus stationarity demands <s> \propto L^2, i.e. we have the scaling law D(2-\tau)=2, which is satisfied by a change in \tau to 1.11. D is the same as in the original. The paper also investigates the universality of the oslo model. It can be mapped exactly onto a depinning model (this is not universality in my opinion, since it is an exact mapping). The mapping onto a 1-d OFC model requires a change of one boundary and a change in the relaxation rules (now always relax to 0 force). Still, the exponents of the original are unchanged in this case. The author interprets this as evidence for a large oslo universality class. The model he presents is not in the same universality class according to his definition, since \tau is different. One could argue that \tau is simply not a universal exponent and that instead D determines the universality class. This makes sense because D is very robust indeed. But the author doesn't take this view.},
	Pages = {4169--4174},
	Title = {Tilted sandpiles, interface depinning, and earthquake models},
	Volume = 59,
	Year = 1999}

