@article{Grassberger79,
	Author = {Peter Grassberger and A. de la Torre},
	Journal = {Ann. Phys.},
	Optnote = {This is a paper about Reggeon Field Theory. It is reported that the theory, though formulated like a quantum field theory, is rather a Markov process, called a ''Gribov process''. This Gribov process involves diffusion, absorption (X \to 0) of an entity, autocatalytic reproduction of entities (X \to 2X), the inverse of autocatalytic reproduction (2X \to X), and recombination (2X \to 0). This model is then put on a lattice by defining two processes: I)particles disappear at rate k and II) particles produce an offspring at a neighbouring site at rate k'. This constitutes an example of a Gribov process. Note that all Gribov steps can be reproduced with this simple model. There's also a Reggeon spin model which simply maps onto the same process by identifying up-spin with occupation (and vice versa). Note that all these models have one absorbing state: the vacuum. (This is pointed out in the paper, referring to the term ''absorbing state'' as Markov chain terminology.) The model is then studied analytically and numerically, obtaining scaling relations and exponents in agreement with published values for Reggeon theory. The numerical treatment is very thourough: Several different methods are used to find exponents, enabling consistency-checks. The model is then modified in several ways, e.g. always producing two offspings, if more than one particle appear at one site then they are deleted or replaced by one etc. None of these changes affect the critical behaviour. The authors speculate that since the investigated type of (discrete) RFT is apparently part of a large universality class, continuous RFT might also be part of it. There is no mention of the conjecture that all models with one absorbing state fall in the same universality class (that of directed percolation (DP)). Neither do the authors mention that RFT and DP are in the same universality class. This conjecture came later (in 1981/1982 by Janssen/ Grassberger respectively).},
	Pages = {373--396},
	Title = {Reggeon Field Theory (Schl{\"o}gl's First Model) on a Lattice: Monte Carlo Calculations of Critical Behaviour},
	Volume = 122,
	Year = 1979}

